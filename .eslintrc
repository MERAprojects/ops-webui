{
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "ecmaFeatures": {
        "jsx": true
    },
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "describe": true,
        "it": true,
        "expect": true,
        "beforeEach": true,
        "afterEach": true,
        "jasmine": true,
        "Immutable": true,
        "AjaxRespondWith": true,
        "AjaxStubRequest": true,
    },
    "rules": {
        "no-mixed-requires": 0,

        "block-scoped-var": 2,
        "consistent-return": 2,
        "curly": 2,
        "dot-notation": 2,
        "dot-location": [2, "property"],
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-alert": 0,
        "no-caller": 2,
        "no-else-return": 2,
        "no-empty-label": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implied-eval": 2,
        "no-iterator": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 2,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-octal-escape": 2,
        "no-octal": 2,
        "no-param-reassign": 2,
        "no-process-env": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unused-expressions": 2,
        "no-void": 2,
        "no-with": 2,
        "radix": 2,
        "wrap-iife": 2,
        "yoda": 0,

        "strict": 0,

        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow-restricted-names": 2,
        "no-shadow": 2,
        "no-undef-init": 2,
        "no-undefined": 2,
        "no-unused-vars": 1,
        "no-use-before-define": 2,

        "brace-style": [ 2, "1tbs", { "allowSingleLine": true } ],
        "camelcase": 2,
        "comma-spacing": 2,
        "comma-style": 2,
        "computed-property-spacing": 0,
        "eol-last": 0,
        "indent": [ 2, 4, { "indentSwitchCase": true } ],
        "key-spacing": 2,
        "new-cap": 0,
        "new-parens": 2,
        "newline-after-var": 0,
        "no-array-constructor": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-new-object": 2,
        "no-spaced-func": 2,
        "no-trailing-spaces": 2,
        "no-unneeded-ternary": 2,
        "object-curly-spacing": [2, "always"],
        "quotes": [2, "single"],
        "semi-spacing": 2,
        "semi": [2, "always"],
        "space-after-keywords": 2,
        "space-before-blocks": 2,
        "space-before-function-paren": [2, "never"],
        "space-infix-ops": 0,
        "space-return-throw-case": 2,
        "space-unary-ops": 2,

        "react/display-name": 2,
        "react/jsx-boolean-value": 2,
        "react/jsx-quotes": 2,
        "react/jsx-no-undef": 2,
        "react/jsx-sort-props": 0,
        "react/jsx-sort-prop-types": 0,
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/no-did-mount-set-state": 2,
        "react/no-did-update-set-state": 2,
        "react/no-multi-comp": 0,
        "react/no-unknown-property": 2,
        "react/prop-types": 1,
        "react/react-in-jsx-scope": 2,
        "react/self-closing-comp": 2,
        "react/sort-comp": 2,
        "react/wrap-multilines": 2
    }
}
